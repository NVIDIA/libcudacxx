#ifndef _CUDA_TUPLE
#define _CUDA_TUPLE

#if defined(_MSC_VER) && !defined(LIBCUDACXX_FORCE_TUPLE)
# error "<tuple> is only supported on GCC and Clang (Override with LIBCUDACXX_FORCE_TUPLE)"
#endif

#include "cstdint"
#include "climits"

#include "type_traits"

#include "detail/libcxx/include/__tuple"

#include "functional"
#include "utility"

#include "detail/__config"

#include "detail/__pragma_push"

#include "detail/libcxx/include/tuple"

#include "detail/__pragma_pop"

#endif //_CUDA_TUPLE


